{
  "apiVersion": 2,
  "name": "hm-pro/container",
  "title": "HM Container",
  "category": "hm-pro",
  "icon": "screenoptions",
  "description": "A layout container that can hold other blocks.",
  "supports": {
    "html": false,
    "align": ["wide", "full"],
    "spacing": {
      "padding": true,
      "margin": true
    }
  },
  "attributes": {
    "isFullWidth": {
      "type": "boolean",
      "default": false
    },
    "columnCount": {
      "type": "number",
      "default": 0
    }
  },
  "editorScript": "file:../../build/editor.js",
  "editorStyle": "file:../../build/editor.css",
  "style": "file:../../build/style.css"
}
